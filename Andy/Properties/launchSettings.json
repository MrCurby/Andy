{
  "profiles": {
    "Electron.NET App": {
      "commandName": "Executable",
      "executablePath": "cmd.exe",
      "commandLineArgs": "/k \"start-electron.bat\"",
      "workingDirectory": "$(ProjectDir)",
      "launchBrowser": false
    },

    "http": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": true,
      "applicationUrl": "http://localhost:5288",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "https": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": true,
      "applicationUrl": "https://localhost:7226;http://localhost:5288",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    }
  }
}
